{
    "name": "hyraiq/abnlookup",
    "description": "ABN validation and verification using the ABR web services API",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/serializer": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/http-client": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.3",
        "vimeo/psalm": "^4.14",
        "phpstan/phpstan": "^1.2",
        "phpmd/phpmd": "^2.11",
        "fakerphp/faker": "^1.17"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyra\\AbnLookup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyra\\Tests\\AbnLookup\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
