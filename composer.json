{
    "name": "hyraiq/abnlookup",
    "description": "ABN validation and verification using the ABR web services API",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/validator": "^5.0",
        "symfony/http-client": "^5.0",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/cache": "^5.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.3",
        "vimeo/psalm": "^4.14",
        "phpstan/phpstan": "^1.2",
        "phpmd/phpmd": "^2.11",
        "fakerphp/faker": "1.14.1",
        "symfony/flex": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyra\\AbnLookup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyra\\Tests\\AbnLookup\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.0"
        }
    }
}
